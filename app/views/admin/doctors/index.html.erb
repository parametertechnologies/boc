<header>
  <h1>Doctors</h1>
  <ul class="actions">
    <li><%= link_to "Add new doctor", new_admin_doctor_path, class: "new" %></li>
  </ul>
</header>

<div id="doctors">
  <% @doctors.each do |doctor| %>
      <h2><%= link_to doctor.last_name, doctor_path(doctor) %> </h2>
      <div id="doctor">
        <%= doctor.full_name %></br>
        <%= doctor.speciality %></br>
        <%= doctor.contact_phone %></br>
      </div>
  <% end %>
</div>
